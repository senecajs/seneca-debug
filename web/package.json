{
  "name": "@seneca/debug",
  "version": "1.0.1",
  "description": "Seneca plugin for debugging Seneca-based systems.",
  "main": "dist/debug.js",
  "scripts": {
    "watch": "webpack --watch",
    "test": "jest --coverage",
    "coveralls": "lab -s -P test -r lcov | coveralls",
    "clean": "rm -rf node_modules yarn.lock package-lock.json",
    "reset": "npm run clean && npm install && npm test",
    "repo-tag": "REPO_VERSION=`node -e \"console.log(require('./package').version)\"` && echo TAG: v$REPO_VERSION && git commit -a -m v$REPO_VERSION && git push && git tag v$REPO_VERSION && git push --tags;",
    "repo-publish": "npm run clean && npm i && npm run repo-publish-quick",
    "repo-publish-quick": "npm run build && npm run test && npm run repo-tag && npm publish --access public --registry https://registry.npmjs.org ",
    "prettier": "prettier --write --no-semi --single-quote *.js lib/*.js test/*.js src/*.ts",
    "build": "tsc -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voxgig/seneca-debug.git"
  },
  "author": "Richard Rodger (richardrodger.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/voxgig/seneca-debug/issues"
  },
  "homepage": "https://github.com/voxgig/seneca-debug#readme",
  "files": [
    "LICENSE",
    "README.md",
    "debug.js",
    "lib/errors.js",
    "dist"
  ],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@seneca/repl": "^4.0.1",
    "babel-runtime": "^6.26.0",
    "d3": "7.4.4",
    "d3-flame-graph": "4.1.3",
    "express": "^4.18.1",
    "material-design-icons-iconfont": "^5.0.1",
    "typescript": "^4.7.3",
    "vue": "^2.6.10",
    "vue-json-pretty": "^1.6.0",
    "vuetify": "^1.5.14",
    "ws": "^8.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/ws": "^8.5.3",
    "babel-loader": "8",
    "code": "5",
    "coveralls": "3",
    "css-loader": "^3.1.0",
    "esbuild": "^0.14.43",
    "esbuild-jest": "^0.5.0",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "3",
    "jest": "^28.1.1",
    "lab": "^18.0.2",
    "prettier": "1",
    "url-loader": "^2.1.0",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "4",
    "vue-template-compiler": "^2.6.10",
    "webpack": "4",
    "webpack-cli": "3",
    "webpack-merge": "4",
    "webpack-node-externals": "1"
  }
}
